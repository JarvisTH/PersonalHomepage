<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>com.jarvis.servlet.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/Servlet/UserLoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserFileServlet</servlet-name>
        <servlet-class>com.jarvis.servlet.UserFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserFileServlet</servlet-name>
        <url-pattern>/Servlet/UserFileServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProjectFileServlet</servlet-name>
        <servlet-class>com.jarvis.servlet.ProjectFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProjectFileServlet</servlet-name>
        <url-pattern>/Servlet/ProjectFileServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SkillFileServlet</servlet-name>
        <servlet-class>com.jarvis.servlet.SkillFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SkillFileServlet</servlet-name>
        <url-pattern>/Servlet/SkillFileServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserCodeLogin</servlet-name>
        <servlet-class>com.jarvis.servlet.UserCodeLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserCodeLogin</servlet-name>
        <url-pattern>/Servlet/UserCodeLogin</url-pattern>
    </servlet-mapping>
</web-app>
